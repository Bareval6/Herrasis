<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btnAbrirAgregarEquipo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Agregar Equipo"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="204dp" />

    <Button
        android:id="@+id/btnAbrirBuscarEquipo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Buscar Equipo"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="264dp" />

    <Button
        android:id="@+id/btnAbrirBorrarEquipo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Borrar Equipo"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="327dp" />

    <Button
        android:id="@+id/btnAbrirListadoEquipos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Listado de Equipos"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="392dp" />

</androidx.constraintlayout.widget.ConstraintLayout>